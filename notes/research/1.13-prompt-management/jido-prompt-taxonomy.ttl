@base <https://jido.ai/ontology#> .
@prefix jido: <https://jido.ai/ontology#> .
@prefix owl:  <http://www.w3.org/2002/07/owl#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

#################################################################
# Ontology Header
#################################################################

jido:JidoPromptTaxonomyOntology
    a owl:Ontology ;
    rdfs:label "Jido Prompt Taxonomy Ontology" ;
    rdfs:comment "Taxonomy module defining prompt categories for multi-level classification within JidoCode." ;
    owl:imports <https://jido.ai/ontology#JidoPromptOntology> ;
    owl:versionInfo "1.0.0" .

#################################################################
# Category Scheme
#################################################################

jido:JidoPromptCategoryScheme a jido:PromptCategoryScheme, skos:ConceptScheme ;
    rdfs:label "Jido Prompt Category Scheme" ;
    rdfs:comment "The default category scheme for JidoCode prompts." ;
    skos:hasTopConcept jido:DomainCategories, jido:TaskCategories, 
                       jido:TechniqueCategories, jido:ComplexityCategories,
                       jido:AudienceCategories .

#################################################################
# Top-Level Domain Categories
#################################################################

jido:DomainCategories a jido:PromptCategory ;
    skos:prefLabel "Domain Categories" ;
    skos:topConceptOf jido:JidoPromptCategoryScheme .

# Coding Domain Hierarchy
jido:CodingDomain a jido:DomainCategory ;
    skos:prefLabel "Coding" ;
    skos:broader jido:DomainCategories ;
    skos:inScheme jido:JidoPromptCategoryScheme .

jido:CodeGenerationDomain a jido:DomainCategory ;
    skos:prefLabel "Code Generation" ;
    skos:broader jido:CodingDomain ;
    skos:inScheme jido:JidoPromptCategoryScheme .

jido:CodeReviewDomain a jido:DomainCategory ;
    skos:prefLabel "Code Review" ;
    skos:broader jido:CodingDomain ;
    skos:inScheme jido:JidoPromptCategoryScheme .

jido:DebuggingDomain a jido:DomainCategory ;
    skos:prefLabel "Debugging" ;
    skos:broader jido:CodingDomain ;
